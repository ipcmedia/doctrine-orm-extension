{
    "name": "ipcmedia/doctrine-orm-provider",
    "description": "Doctrine ORM provider for Silex",
    "keywords": ["silex", "doctrine", "orm", "database", "extension", "provider"],
    "homepage": "https://github.com/ipcmedia/doctrine-orm-extension",
    "authors": [
        { "name": "Radek Simko", "email": "radek_simko@ipcmedia.com" },
        { "name": "Marc Jakubowski", "homepage": "http://nutwerk.de/" }
    ],
    "require": {
        "php": ">=5.3.2",
        "doctrine/orm": ">=2.1.0,<=2.2.3",
        "doctrine/common": ">=2.1.0,<=2.2.3",
        "doctrine/dbal": ">=2.1.0,<=2.2.3"
    },
    "suggest": {
        "symfony/console": "Necessary if you would like to use the Doctrine CLI tools"
    },
    "autoload": {
        "psr-0": { "IpcMedia\\Provider": "lib/" }
    }
}